{
	"name": "n8n-nodes-nvidia-nim",
	"version": "2.1.0",
	"description": "n8n community node for NVIDIA NIM - Complete AI Agent with ReAct pattern, intelligent tool calling, and memory management",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"nvidia",
		"nim",
		"ai",
		"llm",
		"chat",
		"embeddings",
		"machine-learning",
		"langchain",
		"ai-agent"
	],
	"license": "MIT",
	"homepage": "https://github.com/Akash9078/n8n-nodes-nvidia-nim",
	"author": {
		"name": "Akash Kumar Naik",
		"email": "akashkumarnaik948@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Akash9078/n8n-nodes-nvidia-nim.git"
	},
	"engines": {
		"node": ">=18.17.0"
	},
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials",
		"lintfix": "eslint nodes credentials --fix",
		"prepublishOnly": "npm run build && npm run lint"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/NvidiaNimApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/NvidiaNim/NvidiaNim.node.js",
			"dist/nodes/NvidiaNim/LmChatNvidiaNim.node.js",
			"dist/nodes/NvidiaNim/NvidiaNimAgent.node.js"
		]
	},
	"devDependencies": {
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.42.0",
		"eslint-plugin-n8n-nodes-base": "^1.14.0",
		"gulp": "^4.0.2",
		"n8n-workflow": "^1.0.0",
		"prettier": "^2.7.1",
		"typescript": "^5.0.0"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"dependencies": {
		"@langchain/core": "^0.3.78",
		"@langchain/openai": "^0.6.14",
		"langchain": "^0.3.35"
	}
}
